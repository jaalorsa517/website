{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "useDefineForClassFields": true,
    "skipDefaultLibCheck": true,
    "noEmit": true,
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "types": ["vite/client", "vite-plugin-pwa/client"],
    "baseUrl": "./",
    "paths": {
      "@/*": ["src/*"]
    },
    "lib": ["esnext", "dom", "dom.iterable", "scripthost"],
    "jsx": "preserve"
  },
  "include": ["src/**/*.js", "src/**/*.vue", "src/main.ts", "src/router/index.ts", "src/router/routes.ts", "src/services/converter.ts", "src/services/ga.services.ts", "src/services/http.services.ts", "src/services/youtube.service.ts", "src/shared/const.ts", "src/shared/youtube.dto.ts", "src/store/firebaseStore.ts", "src/store/index.ts", "src/utils/handlerSeo.ts", "src/utils/index.ts"]
}
